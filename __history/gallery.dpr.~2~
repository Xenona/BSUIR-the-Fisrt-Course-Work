program gallery;

uses
  Vcl.Forms,
  lalala in 'lalala.pas' {Form1},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Smokey Quartz Kamri');
  Application.CreateForm(TFGallery, FGallery);
  Application.Run;
end.
